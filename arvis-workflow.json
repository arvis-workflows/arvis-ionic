{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "defaultIcon": "icon.png",
  "category": "",
  "name": "arvis-ionic",
  "creator": "Sam Verschueren",
  "description": "Search through the Ionic documentation",
  "version": "0.0.2",
  "webAddress": "https://github.com/arvis-workflows/arvis-ionic",
  "readme": "",
  "enabled": true,
  "commands": [
    {
      "type": "scriptFilter",
      "command": "ion",
      "title": "Search through the Ionic documentation",
      "subtitle": "",
      "scriptFilter": "node index.js \"{query}\"",
      "runningSubtext": "Searching...",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "open",
          "target": "{query}"
        }
      ]
    }
  ]
}
